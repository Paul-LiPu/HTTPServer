<?xml version="1.0" encoding="UTF-8"?>

 <web-app>
	 <servlet>
	 	<servlet-name>login</servlet-name>
	 	<servlet-class>me.lipu.webapp.LoginServlet</servlet-class>
	 </servlet>
	 
	 <servlet-mapping>
	 	<servlet-name>login</servlet-name>
	 	<url-pattern>/login</url-pattern> 
	 </servlet-mapping>
	 
	 	 
	 <servlet>
	 	<servlet-name>reg</servlet-name>
	 	<servlet-class>me.lipu.webapp.RegisterServlet</servlet-class>
	 </servlet>
	 
	 <servlet-mapping>
	 	<servlet-name>reg</servlet-name>
	 	<url-pattern>/reg</url-pattern> 
	 </servlet-mapping>
	 
	 <filter>
	 	<filter-name>servletInfo</filter-name>
	 	<filter-class>me.lipu.webapp.PrintServletFilter</filter-class>
	 </filter>
 </web-app>